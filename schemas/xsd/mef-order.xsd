<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:mef-order="http://cableapi.cablelabs.com/schemas/v2" elementFormDefault="qualified" targetNamespace="http://cableapi.cablelabs.com/schemas/v2">
  <xs:element name="mef-order" type="mef-order:mef-order"/>
  <xs:complexType name="mef-order">
    <xs:sequence>
      <xs:element minOccurs="0" name="order" type="mef-order:order"/>
      <xs:element minOccurs="0" name="order-create-update" type="mef-order:order-create-update"/>
      <xs:element minOccurs="0" name="orderNotification" type="mef-order:orderNotification"/>
      <xs:element minOccurs="0" name="orderStatus" type="mef-order:orderStatus"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="accessEplOrderItemDetail">
    <xs:sequence>
      <xs:element minOccurs="1" name="action">
        <xs:annotation>
          <xs:documentation>The action to be taken for this order item (E.g. Install, Change).</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="INSTALL"/>
            <xs:enumeration value="CHANGE"/>
            <xs:enumeration value="NO_CHANGE"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="sellerOvcId">
        <xs:annotation>
          <xs:documentation>The circuit identifier that the Seller uses to refer to this OVC.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="45"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="1" name="buyerOvcId">
        <xs:annotation>
          <xs:documentation>The circuit identifier that the Partner/Access Provider (Buyer) uses to refer to this OVC.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="45"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="productSpecification">
        <xs:annotation>
          <xs:documentation>An identifier of the Partner/Access Provider (Seller) Product Specification (product catalog item) that the buyer is ordering at the Access-EPL level.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="25"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="enniSvlanId">
        <xs:annotation>
          <xs:documentation>The S-VLAN ID to be used for this service or the 1st value requested when a range is used.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:int">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="4095"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="enniSvlanIdLast">
        <xs:annotation>
          <xs:documentation>The last S-VLAN ID to be used for this service when a range is requested.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:int">
            <xs:minInclusive value="1"/>
            <xs:maxInclusive value="4095"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="mtuSize">
        <xs:annotation>
          <xs:documentation>The maximum transmission unit size specifies the maximum length ENNI Frame in bytes allowed at the ENNI. An integer greater or equal to 1526.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:int">
            <xs:minInclusive value="1526"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="colorForwardingEnabled" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>Color Forwarding is an OVC attribute defining the relationship between the color on an egress ENNI frame and the color of the corresponding ingress ENNI Frame or Service Frame.  For ordering, this attribute identifies if the Buyer requests Color Forwarding for this service</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="classOfService">
        <xs:annotation>
          <xs:documentation>The name of the service level for this Access EPL. (E.g. Gold).</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="20"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="enniReference">
        <xs:annotation>
          <xs:documentation>A reference to an existing or ordered ENNI.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:choice>
            <xs:element name="orderedEnniReference" type="mef-order:orderedEnniReference"/>
            <xs:element name="existingEnniReference" type="mef-order:existingEnniReference"/>
          </xs:choice>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" name="enniSiteContact" type="mef-order:contact">
        <xs:annotation>
          <xs:documentation>The primary site contact for the ENNI site.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="enniSiteAlternateContact" type="mef-order:contact">
        <xs:annotation>
          <xs:documentation>The secondary site contact for the ENNI site.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="enniIngressBandwidthProfile" type="mef-order:bandwidthProfile">
        <xs:annotation>
          <xs:documentation>The ingress bandwidth profile at the ENNI end of the OVC that implements the Access EPL.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="uniReference">
        <xs:annotation>
          <xs:documentation>A reference to an existing or ordered UNI.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:choice>
            <xs:element name="orderedUniReference" type="mef-order:orderedUniReference"/>
            <xs:element name="existingUniReference" type="mef-order:existingUniReference"/>
          </xs:choice>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" name="uniSiteContact" type="mef-order:contact">
        <xs:annotation>
          <xs:documentation>The primary site contact for the UNI site. The UNI site contacts are ONLY required when the UNI is NOT on the same Order as the Access EPL.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="uniSiteAlternateContact" type="mef-order:contact">
        <xs:annotation>
          <xs:documentation>The secondary site contact for the UNI site. The UNI site contacts are ONLY required when the UNI is NOT on the same Order as the Access EPL.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="uniIngressBandwidthProfile" type="mef-order:bandwidthProfile">
        <xs:annotation>
          <xs:documentation>The ingress bandwidth profile at the UNI end of the OVC that implements the Access EPL.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="addressReference">
    <xs:sequence>
      <xs:element minOccurs="0" name="referenceType">
        <xs:annotation>
          <xs:documentation>The type of the reference.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="25"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="reference">
        <xs:annotation>
          <xs:documentation>The reference can be used to identify an address.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="60"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="bandwidthProfile">
    <xs:sequence>
      <xs:element minOccurs="0" name="committedInformationRate" type="mef-order:informationRateQuantity">
        <xs:annotation>
          <xs:documentation>The ingress bandwidth profile at the ENNI end of the OVC that implements the Access EPL.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="committedBurstSize">
        <xs:annotation>
          <xs:documentation>Limits the maximum number of bytes available for a burst of Service or ENNI Frames sent at the EI speed to remain CIR- onformant. MUST be &gt;= 12176.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:int">
            <xs:minInclusive value="0"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="excessInformationRate" type="mef-order:informationRateQuantity">
        <xs:annotation>
          <xs:documentation>If specified, defines the average rate in bits/s of Service or ENNI Frames up to which the network may deliver Service or Network Frames but without any performance objectives.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="excessBurstSize">
        <xs:annotation>
          <xs:documentation>If specified, limits the maximum number of bytes available for a burst of Service or ENNI Frames sent at the EI speed to remain EIR-conformant.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:int">
            <xs:minInclusive value="0"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="colorMode">
        <xs:annotation>
          <xs:documentation>Color Mode parameter indicates whether the color-aware or color-blind property is employed by the Bandwidth Profile.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="COLOR_AWARE"/>
            <xs:enumeration value="COLOR_BLIND"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="couplingFlag" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>The Coupling Flag allows the choice between two modes of operation of the Bandwidth Profile algorithm.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="billingInformation">
    <xs:sequence>
      <xs:element minOccurs="0" name="existingBillingAccountNumber">
        <xs:annotation>
          <xs:documentation>Identifies an existing billing account to which the recurring and non-recurring charges for this request will be billed. Must be specified if billing contact and billing address are not specified</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="13"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="billingContact" type="mef-order:contact">
        <xs:annotation>
          <xs:documentation>Identifies the information of the person or office to be contacted on billing matters. Used to establish a new billing account. Must be specified if existingBillingAccountNumber is not specified.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="billingAddress">
        <xs:annotation>
          <xs:documentation>Identifies a property address or postal delivery address. Used to establish a new billing account. Must be specified if existingBillingAccountNumber is not specified.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:choice>
            <xs:element name="formattedAddress" type="mef-order:formattedAddress"/>
            <xs:element name="fieldedAddress" type="mef-order:fieldedAddress"/>
            <xs:element name="addressReference" type="mef-order:addressReference"/>
          </xs:choice>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="contact">
    <xs:sequence>
      <xs:element minOccurs="1" name="name">
        <xs:annotation>
          <xs:documentation>Identifies the name of the person or office.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="15"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="1" name="telephoneNumber">
        <xs:annotation>
          <xs:documentation>Identifies the telephone number (excluding extension) of the Contact.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="15"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="telephoneNumberExt">
        <xs:annotation>
          <xs:documentation>Identifies the telephone number extension of the Contact.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="6"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="email">
        <xs:annotation>
          <xs:documentation>Identifies the electronic mail address of the Contact.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="60"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="disconnectOrderItemDetail">
    <xs:sequence>
      <xs:element minOccurs="1" name="action">
        <xs:annotation>
          <xs:documentation>The action to be taken for this order item (E.g. Install, Change).</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="DISCONNECT"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="1" name="sellerServiceOrEndpointId">
        <xs:annotation>
          <xs:documentation>One or more of the following Seller-assigned values may be included: OVC Identifier, EVC Identifier, UNI Identifier, ENNI Identifier.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="45"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="buyerServiceOrEndpointId">
        <xs:annotation>
          <xs:documentation>One or more of the following Buyer-assigned values may be included: Buyer OVC Identifier, Buyer EVC Identifier, Buyer UNI Identifier, Buyer ENNI Identifier.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="45"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="existingEnniReference">
    <xs:sequence>
      <xs:element minOccurs="1" name="sellerEnniId">
        <xs:annotation>
          <xs:documentation>The circuit identifier that the seller uses to refer to this ENNI.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="45"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="buyerEnniId">
        <xs:annotation>
          <xs:documentation>The circuit identifier that the buyer uses to refer to this ENNI.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="45"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="existingUniReference">
    <xs:sequence>
      <xs:element minOccurs="1" name="sellerUniId">
        <xs:annotation>
          <xs:documentation>The circuit identifier that the seller uses to refer to this UNI.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="45"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="buyerUniId">
        <xs:annotation>
          <xs:documentation>The circuit identifier that the buyer uses to refer to this UNI.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="45"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="fieldedAddress">
    <xs:sequence>
      <xs:element minOccurs="0" name="streetNr">
        <xs:annotation>
          <xs:documentation>Number identifying a specific property on a public street. It may be combined with streetNrLast for ranged addresses.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="10"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="streetNrSuffix">
        <xs:annotation>
          <xs:documentation>The suffix of the first street number (in a street number range) or the suffix for the street number if there is no range.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="10"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="streetNrLast">
        <xs:annotation>
          <xs:documentation>Last number in a range of street numbers allocated to an address.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="10"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="streetName">
        <xs:annotation>
          <xs:documentation>The name of the street or other street type.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="60"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="streetType">
        <xs:annotation>
          <xs:documentation>The type of street (e.g., Street, St., Road, Rd., Alley).</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="25"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="streetSuffix">
        <xs:annotation>
          <xs:documentation>A modifier denoting a relative direction.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="2"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="subUnitType">
        <xs:annotation>
          <xs:documentation>The type of subunit (e.g., Flat, Suite, Tower, Unit).</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="25"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="subUnitNr">
        <xs:annotation>
          <xs:documentation>The discriminator used for the subunit, often just a sumple number but may also be a range.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="10"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="levelType">
        <xs:annotation>
          <xs:documentation>Describes level types within a building (e.g., Floor).</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="25"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="levelNr">
        <xs:annotation>
          <xs:documentation>Used where a level type may be repeated (e.g., BASEMENT 1, BASEMENT 2).</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="25"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="buildingName">
        <xs:annotation>
          <xs:documentation>Allows for buildings with well-known names to be used in an address.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="60"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="privateStreetName">
        <xs:annotation>
          <xs:documentation>Private streets internal to a property (e.g., a university) with an internal name not recorded by the land title office.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="60"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="locality">
        <xs:annotation>
          <xs:documentation>A bounded locality within a city, town or shire principally of urban character.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="50"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="city">
        <xs:annotation>
          <xs:documentation>The city that the address is in.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="50"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="stateOrProvince">
        <xs:annotation>
          <xs:documentation>The state or province that the address is in.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="50"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="postcode">
        <xs:annotation>
          <xs:documentation>A descriptor for a postal delivery area, used to speed and simplify the delivery of mail.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="6"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="postcodeExtension">
        <xs:annotation>
          <xs:documentation>The four-digit extension on an American postal code, what comes after the hyphen when specified.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="4"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="country">
        <xs:annotation>
          <xs:documentation>The country that the address is in.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="50"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="formattedAddress">
    <xs:sequence>
      <xs:element minOccurs="0" name="addressLine1">
        <xs:annotation>
          <xs:documentation>A formatted address line that contains a non-fielded street address.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="60"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="addressLine2">
        <xs:annotation>
          <xs:documentation>A formatted address line that contains a non-fielded street address, sub-address (e.g. Suite number), etc.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="60"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="locality">
        <xs:annotation>
          <xs:documentation>A bounded locality within a city, town or shire principally of urban character.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="50"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="city">
        <xs:annotation>
          <xs:documentation>City that the address is in.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
