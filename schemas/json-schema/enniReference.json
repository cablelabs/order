{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "enniReference",
  "id": "enniReference",
  "description": "A reference to an existing or ordered ENNI.",
  "type": "object",
  "required": [
      "objectType",
      "referenceType"
  ],
  "properties": {
    "objectType": {
        "type": "string",
        "enum": [
            "enniReference"
        ],
        "description": "The type of the object, enniReference."
    },
    "referenceType": {
      "type": "string",
      "enum": [
        "EXISTING",
        "ORDERED"
      ],
      "description": "The type of the reference."
    },
    "enniReference": {
      "type": "object",
      "description": "A reference to an existing or ordered ENNI.",
      "oneOf": [
        {
          "$ref": "orderedEnniReference.json#"
        },
        {
          "$ref": "existingEnniReference.json#"
        }
      ]
    }
  }
}
