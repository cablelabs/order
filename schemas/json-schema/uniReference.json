{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "uniReference",
  "id": "uniReference",
  "description": "A reference to an existing or ordered UNI.",
  "type": "object",
  "required": [
      "objectType",
      "referenceType"
  ],
  "properties": {
    "objectType": {
        "type": "string",
        "enum": [
            "uniReference"
        ],
        "description": "The type of the object, uniReference."
    },
    "referenceType": {
      "type": "string",
      "enum": [
        "EXISTING",
        "ORDERED"
      ],
      "description": "The type of the reference."
    },
    "uniReference": {
      "type": "object",
      "description": "A reference to an existing or ordered UNI.",
      "oneOf": [
        {
          "$ref": "orderedUniReference.json#"
        },
        {
          "$ref": "existingUniReference.json#"
        }
      ]
    }
  }
}
